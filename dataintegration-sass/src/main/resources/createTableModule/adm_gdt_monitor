create table %s.adm_gdt_monitor (
`log_type`				STRING	COMMENT '10：广点通',
`log_version`			STRING	COMMENT '1',
`log_time`				STRING	COMMENT '秒级时间戳',
`event_type`			STRING	COMMENT  '0:点击,1 企微转化数,2 公众号转化数',
`user_id`				STRING	COMMENT '用户id',
`muid`					STRING	COMMENT '设备id，由IMEI（Android应用）md5生成，或是由IDFA（iOS应用）md5生成；',
`click_time`			STRING	COMMENT '点击发生的时间，由腾讯社交广告系统生成，取值为标准时间戳，秒级别',
`click_id`				STRING	COMMENT '腾讯社交广告后台生成的点击id，腾讯社交广告系统中标识用户每次点击生成的唯一标识',
`appid`					STRING	COMMENT 'Android应用为应用宝移动应用的id，或者iOS应用在Apple App Store的id；创建转化时，需填入此appid',
`advertiser_id`			STRING	COMMENT '广告主在腾讯社交广告（e.qq.com）的账户id',
`app_type`				STRING	COMMENT 'android，ios',
`android_id`			STRING	COMMENT '由android_idmd5生成，当设备id获取不到或匹配不上时，可以用于作为归因补充；',
`mac`					STRING	COMMENT "由mac地址去掉\":\",大写后MD5生成,当设备id获取不到或匹配不上时,可以用于作为归因补充",
`ip`					STRING	COMMENT 'IP地址，不加密，当设备号获取不到或匹配不上时，可以用于作为归因补充；',
`user_agent`			STRING	COMMENT 'urlencode，当设备号获取不到或匹配不上时，可以用于作为归因补充；',
`campaign_id`			STRING	COMMENT '推广计划id',
`adgroup_id`			STRING	COMMENT '广告id',
`creative_id`			STRING	COMMENT '创意id',
`agent_id`				STRING	COMMENT '代理商id',
`deeplink_url`			STRING	COMMENT '应用直达链接',
`dest_url`				STRING	COMMENT '落地页链接',
`device_os_type`		STRING	COMMENT '设备系统，ios、android',
`click_sku_id`			STRING	COMMENT 'DPA商品id',
`process_time`			STRING	COMMENT 'int',
`product_type`			STRING	COMMENT '商品类型',
`request_id`			STRING	COMMENT '请求id',
`site_set`				STRING	COMMENT '站点集id',
`adgroup_name`			STRING	COMMENT '广告名称',
`oaid`					STRING	COMMENT '安卓系统移动终端补充设备标识',
`id`					STRING	COMMENT '唯一id 用于区分app、web，类似站点',
`account_id`			STRING	COMMENT 'jnos组织ID',
`campaign_name`			STRING	COMMENT '推广计划名称',
`wechat_unionid`		STRING	COMMENT '',
`wechat_openid`			STRING	COMMENT '',
`trace_id`				STRING	COMMENT '',
`ad_id`					STRING	COMMENT '微信广告tid',
`canvas_id`				STRING	COMMENT '微信广告原生页id',
`act_time`				STRING	COMMENT '关注时间',
`position_id`			STRING	COMMENT '广告位',
`wechat_account_id`		STRING	COMMENT '微信广告appid',
`qywx_corp_id`			STRING	COMMENT '微信企业主体信息（CorpID）',
`add_channel`			STRING	COMMENT '企业微信添加渠道',
`task_id`				STRING	COMMENT '任务id'
)COMMENT '腾讯回流数据'
PARTITIONED BY (ds string COMMENT '分区字段(yyyy-mm-dd)')
 STORED AS ORC TBLPROPERTIES ('orc.compress'='SNAPPY','SENSITIVE_TABLE'='TRUE')