create table %s.adm_ad_monitor (
`log_type`				STRING	COMMENT '12：TD 移动App广告监测',
`log_version`			STRING	COMMENT '1',
`log_time`				STRING	COMMENT '秒级时间戳',
`event_type`			STRING	COMMENT '0:曝光 1点击',
`click_time`			STRING	COMMENT '秒级时间戳',
`appkey`				STRING	COMMENT 'appkey',
`spread_url`			STRING	COMMENT '推广活动短链接',
`spread_name`			STRING	COMMENT '推广活动名称',
`spread_group`			STRING	COMMENT '推广活动组名称',
`channel_id`			STRING	COMMENT '渠道ID',
`channel_name`			STRING	COMMENT '渠道名称',
`user_agent`			STRING	COMMENT 'user agent',
`ip`					STRING	COMMENT 'ip',
`browser_id`			STRING	COMMENT '浏览器ID',
`fraud_prevention`		STRING	COMMENT '作弊标识',
`media_info`			STRING	COMMENT 'info 有标准字段，有媒体自定义字段，媒体自定义不影响TD，仅适用于媒体',
`imei`					STRING	COMMENT '先取md5 没有取明文',
`idfa`					STRING	COMMENT '先取md5 没有取明文',
`android_id`			STRING	COMMENT 'android_id',
`user_id`				STRING	COMMENT '用户ID',
`site_id`				STRING	COMMENT '站点ID',
`task_id`				STRING	COMMENT '任务ID',
`bdid`					STRING	COMMENT 'bdid',
`account_id`			STRING	COMMENT 'jnos组织ID',
`oaid`					STRING	COMMENT 'oaid',
`brand_name`			STRING	COMMENT '品牌名称',
`product_name`			STRING	COMMENT '产品名称',
`order`					STRING	COMMENT '投放订单',
`activity`				STRING	COMMENT '投放活动',
`creative_name`			STRING	COMMENT '创意名称',
`media_platform`		STRING	COMMENT '媒体点位（对应日志slotName）',
`sku`					STRING	COMMENT 'sku',
`device_name`			STRING	COMMENT '设备品牌',
`device_model`			STRING	COMMENT '设备型号',
`adslot`				STRING	COMMENT '媒体点位（废弃，做保留字段）',
`mac`					STRING	COMMENT 'mac设备号'
)
PARTITIONED BY (ds string COMMENT '分区字段(yyyy-mm-dd)')STORED AS ORC TBLPROPERTIES ('orc.compress'='SNAPPY','SENSITIVE_TABLE'='TRUE')