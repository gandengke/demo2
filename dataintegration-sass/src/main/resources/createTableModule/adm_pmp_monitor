create table %s.adm_pmp_monitor(
`log_type`             	STRING	COMMENT '3：PMP监测',
`log_version`          	STRING	COMMENT '日志版本',
`log_time`             	STRING	COMMENT '日志生成时间,秒级时间戳',
`event_type`           	STRING	COMMENT '0：曝光，1：点击',
`ip`                   	STRING	COMMENT 'IP地址',
`area`                 	STRING	COMMENT '地域ID',
`behe_cookie`          	STRING	COMMENT 'PC和Wap为璧合Cookie，App为设备号大写MD5（IMEI=IDFA>MAC>其他）',
`media_cookie`         	STRING	COMMENT 'PC和Wap为媒体Cookie，App为设备号大写MD5（IMEI=IDFA>MAC>其他）',
`user_id`               STRING	COMMENT '账户ID',
`campaign_id`          	STRING	COMMENT '活动ID',
`order_id`             	STRING	COMMENT '订单ID',
`adid`                 	STRING	COMMENT '广告ID',
`creative_id`          	STRING	COMMENT '创意ID',
`material_id`         	STRING	COMMENT '素材ID',
`material_type`        	STRING	COMMENT '素材分类',
`policy_id`            	STRING	COMMENT '策略ID',
`adx_id`               	STRING	COMMENT 'ADX ID',
`point_id`             	STRING	COMMENT '广告位ID',
`point_type`           	STRING	COMMENT '0：PC图片，1：PC视频，2：App横幅，3：App视频，4：App开屏/插屏，5：App原生，6：Wap图片，7：Wap视频，-1：PC原生',
`url`                  	STRING	COMMENT 'redis人群结果',
`model_id`             	STRING	COMMENT '一般只有原生广告位会填',
`deal_id`              	STRING	COMMENT '只有PMP日志会填',
`bid_time`             	STRING	COMMENT '竞价时间戳',
`bid_unique`           	STRING	COMMENT '竞价唯一标示',
`price`                	STRING	COMMENT '只有曝光日志会填,价格密文',
`media_cost`           	STRING	COMMENT '只有曝光日志会填,媒体花费',
`agent_cost`           	STRING	COMMENT '只有曝光日志会填,代理商花费',
`advertiser_cost`      	STRING	COMMENT '只有曝光日志会填,广告主花费',
`gender`             	STRING	COMMENT '用户性别',
`age`                  	STRING	COMMENT '用户年龄',
`os`                   	STRING	COMMENT '操作系统',
`browser`              	STRING	COMMENT '浏览器',
`site_type`            	STRING	COMMENT 'dmp人群结果',
`app_id`               	STRING	COMMENT '只有App会填',
`freq_level`           	STRING	COMMENT '0：订单，1：活动',
`freq_interval`        	STRING	COMMENT '0：小时，1：天，2：周，3：月，4：全周期',
`freq`                 	STRING	COMMENT '只有曝光日志会填',
`use_behe_cookie`      	STRING	COMMENT '0：否，1：是',
`debug`                	STRING	COMMENT '0：否，1：是',
`product_id`           	STRING	COMMENT '标识是 流量通还是竞价通',
`agent_id`             	STRING	COMMENT '代理商ID',
`schedule_id`          	STRING	COMMENT '排期ID',
`crowd_id`             	STRING	COMMENT '人群包ID',
`order_type`           	STRING	COMMENT '1:正常订单，2:脑图订单',
`brain_type`           	STRING	COMMENT '若43的值为2，则此处填写脑图订单ID，否则为0',
`is_online`            	STRING	COMMENT '1预约投放 2直投 3竞价',
`exchange_campaign_id` 	STRING	COMMENT '非换量活动为空',
`department1_id`       	STRING	COMMENT '默认为0，空部门',
`department2_id`       	STRING	COMMENT '默认为0，空部门',
`account_id`			STRING	COMMENT 'jnos组织ID',
`brand`                	STRING	COMMENT '默认为0，空品牌',
`product`              	STRING	COMMENT '默认为0，空产品',
`behe_point_id`        	STRING	COMMENT '璧合广告位id',
`deal_type`            	STRING	COMMENT '1:pdb 2:pd 3:rtb',
`imei`                 	STRING	COMMENT 'IMEI',
`imei_md5`             	STRING	COMMENT 'IMEI_MD5',
`idfa`                 	STRING	COMMENT 'IDFA',
`idfa_md5`             	STRING	COMMENT 'IDFA_MD5',
`mac`                  	STRING	COMMENT 'MAC',
`mac_md5`              	STRING	COMMENT 'MAC_MD5',
`media_id`             	STRING	COMMENT 'mediaID',
`os_type`	STRING	COMMENT '操作系统类型',
`connect_type`	STRING	COMMENT '网络连接类型',
`make`	STRING	COMMENT '制造厂商,如“apple”“Samsung”“Huawei“，默认为空字符串',
`model`	STRING	COMMENT '型号, 如”iphoneA1530”，默认为空字符串',
`stat_time`	STRING	COMMENT '追数时用到，一般不用'

)COMMENT '媒体回流数据'
PARTITIONED BY (ds string COMMENT '分区字段(yyyy-mm-dd)') STORED AS ORC TBLPROPERTIES ('orc.compress'='SNAPPY','SENSITIVE_TABLE'='TRUE')
